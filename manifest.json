{
    "name": "Badger Automator",
    "description": "Scrape canvas and UW portal schedular website and add class asignements and timings to a calendar",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/logo_temp/16.png",
            "32": "/images/logo_temp/32.png",
            "48": "/images/logo_temp/48.png",
            "128": "/images/logo_temp/128.png"
        }
      },
    "icons": {
        "16": "/images/logo_temp/16.png",
        "32": "/images/logo_temp/32.png",
        "48": "/images/logo_temp/48.png",
        "128": "/images/logo_temp/128.png"
    }
}